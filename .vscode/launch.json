{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "type": "java",
            "name": "Launch AstPrinter",
            "request": "launch",
            "mainClass": "com.interpreter.lox.AstPrinter"
        },
        {
            "type": "java",
            "name": "Launch Current File",
            "request": "launch",
            "mainClass": "${file}"
        },
        {
            "type": "java",
            "name": "Launch App",
            "request": "launch",
            "mainClass": "App",
        },
        {
            "type": "java",
            "name": "Launch Lox",
            "request": "launch",
            "mainClass": "com.interpreter.lox.Lox",
        },
        {
            "type": "java",
            "name": "Launch GenerateAst",
            "request": "launch",
            "mainClass": "com.interpreter.tool.GenerateAst",
            "args": "${workspaceFolder}/src/com/interpreter/lox"
        }
    ]
}